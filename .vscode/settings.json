{
    "sqf.enableCBA": true,
    "sqf.enableACE3": true,
    "files.associations": {
        "*.hpp": "sqf"
    },
    "sqflint.ignoredVariables": [
        "ace_interact_menu_fnc_createAction",
        "ace_interact_menu_fnc_addActionToObject",
        "CBA_fnc_compatibleMagazines",
        "CBA_Settings_fnc_init"
    ],
    "psi-header.lang-config": [
        {
            "license": "Custom",
            "language": "sqf",
            "rootDirFileName": "description.ext",
            "begin": "/*",
            "prefix": "    ",
            "end": "*/",
            "forceToTop": true
        }
    ],
    "psi-header.changes-tracking": {
        "isActive": true,
        "include": ["sqf"],
        "modDate": "Last Update:",
        "modDateFormat": "YYYY-MM-DD",
        "autoHeader": "manualSave"
    },
    "psi-header.license-text": [
        "GNU General Public License v3.0 - https://www.gnu.org/licenses/gpl-3.0.html"
    ],
    "psi-header.templates": [
        {
            "language": "sqf",
            "template": [
                "KP_fnc_",
                "",
                "File: <<filename>>",
                "Author: Dubjunk - https://github.com/KillahPotatoes",
                "Date: <<filecreated('YYYY-MM-DD')>>",
                "Last Update:",
                "License: GNU General Public License v3.0 - https://www.gnu.org/licenses/gpl-3.0.html",
                "",
                "Description:",
                "    No description added yet.",
                "",
                "Parameter(s):",
                "    _localVariable - Description [DATATYPE, defaults to DEFAULTVALUE]",
                "",
                "Returns:",
                "    Function reached the end [BOOL]"
            ]
        }
    ]
}
