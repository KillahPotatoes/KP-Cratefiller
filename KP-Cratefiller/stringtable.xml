            <Key ID="STR_KPCF_LISTGRENADES">
                <English>Grenades</English>
                <German>Granaten</German>
                <Polish>Granaty</Polish>
            </Key>
            <Key ID="STR_KPCF_LISTEXPLOSIVES">
                <English>Explosives</English>
                <German>Sprengstoffe</German>
