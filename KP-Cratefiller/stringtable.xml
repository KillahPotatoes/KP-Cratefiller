            <Key ID="STR_KPCF_IMPORT">
                <English>Import</English>
                <German>Importieren</German>
                <Polish>Importuj</Polish>
            </Key>
            <Key ID="STR_KPCF_LISTWEAPONS">
                <English>Weapons</English>
                <German>Waffen</German>
                <Polish>Bronie</Polish>
